public class Car {
    private String brand;
    private String color;
    private int year;

    public Car(String brand, String color, int year) {
        this.brand = brand;
        this.color = color;
        this.year = year;
    }

    public void displayInfo() {
        System.out.println(year + " " + color + " " + brand);
    }
}
 
